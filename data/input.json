{
    "n_samples": 500,
    "max_iterations": 50,
    "precision_threshold": 0.8,
    "recall_threshold": 0.8,
    "class_to_predict": "non_parsing_error",
    
    "fuzzer_base_instruction": {
        "matchCount": 1,
        "criteria": [
            {
                "packetType": "packet_in",
                "ethType": "arp"
            }
        ],
        "actions": []
    },
    
    "initial_rules": [
        {
            "probability": 0.8,
            "conditions": "(match_type > 1) and (reason < 3)",
            "class": "parsing_error"
        },
        {
            "probability": 0.5,
            "conditions": "(oxm_value > 5) and (match_length < 6)",
            "class": "parsing_error"
        }
    ]
}